	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "0.116.0+1.21.1",
		"fabric-language-kotlin": "*",
		"cobblemon": "1.6.1+1.21.1",
		"trinkets": ">=3.10.0",
