{
	"schemaVersion": 1,
	"id": "cobblemonevolved",
	"version": "${version}",
	"name": "Cobblemon Evolved",
	"description": "It works!",
	"authors": [
		"Serotonin"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/cobblemonevolved/icon.png",
	"environment": "*",
