		"main": [
			{
				"value": "com.serotonin.Cobblemonevolved",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.serotonin.CobblemonevolvedDataGenerator",
				"adapter": "kotlin"
			}
		],
		"client": [
			"com.serotonin.CobblemonevolvedClient"
