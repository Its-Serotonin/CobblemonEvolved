{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:nastyplot_question1",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/nasty_plot_question"
			}
		},
		"curves": {
			"variable.motionleft": {
				"type": "catmull_rom",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [
					0,
					-0.37,
					0.69,
					0
				]
			},
			"variable.motionup": {
				"type": "catmull_rom",
				"input": "v.particle_lifetime",
				"horizontal_range": "v.particle_age",
				"nodes": [
					0,
					0,
					0.54,
					0,
					0,
					0,
					0
				]
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"per_update_expression": "variable.motionleft;variable.motionup;variable.motionright;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 5,
				"max_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 3
			},
			"minecraft:emitter_shape_point": {
				"offset": [
					1,
					1.5,
					0
				]
			},
			"minecraft:particle_lifetime_expression": {
				"expiration_expression": "variable.emitter_age > 2"
			},
			"minecraft:particle_initial_spin": {
				"rotation_rate": 25
			},
			"minecraft:particle_initial_speed": 2,
			"minecraft:particle_motion_parametric": {
				"rotation": "math.cos(variable.particle_age * 360) * 15 "
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					0.3,
					0.3
				],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 9,
					"texture_height": 9,
					"uv": [
						0,
						0
					],
					"uv_size": [
						9,
						9
					]
				}
			},
			"minecraft:particle_appearance_lighting": {}
		}
	}
},
